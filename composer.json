{
    "name": "mihdan/mihdan-yandex-turbo-feed",
    "description": "Mihdan: Yandex Turbo Feed by mihdan â€“ allows you to convert your site materials into Yandex.Turbo format.",
    "type": "wordpress-plugin",
    "license": "GPL2",
    "authors": [
        {
            "name": "mihdan",
            "email": "mikhail@kobzarev.com"
        }
    ],
    "keywords": [
        "mihdan", "composer", "yandex", "turbo", "feed", "wordpress", "wp", "wp-config", "wordpress-plugin"
    ],
    "support": {
        "issues": "https://wordpress.org/support/plugin/mihdan-yandex-turbo-feed"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "reduxframework/redux-framework-4",
                "type": "wordpress-plugin",
                "version": "4.0.2",
                "dist": {
                    "type": "zip",
                    "url": "https://github.com/reduxframework/redux-framework-4/archive/master.zip"
                },
                "require" : {
                    "composer/installers": "*"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "*",
        "reduxframework/redux-framework-4" : "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "sebastian/phpcpd": "^3.0",
        "10up/wp_mock": "~0.3",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^2.9",
        "phpcompatibility/php-compatibility": "^9.0",
        "wp-coding-standards/wpcs": "^1.2",
        "wp-cli/wp-cli": "*"
    },
    "extra": {
        "installer-paths": {
            "vendor/{$name}/": ["type:wordpress-plugin"]
        }
    },
    "autoload": {
        "classmap": [
            "includes",
            "vendor/redux-framework-4/ReduxCore"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "includes"
        ]
    },
    "archive": {
        "exclude": [
            "/vendor/redux-framework-4/*",
            "!/vendor/redux-framework-4/ReduxCore"
        ]
    }
}
